a = int(input())
b = int(input())
c = int(input())
largest = 0
medium = 0
smallest = 0
if a >= b and a >= c:
    if b >= c:
        largest = a
        medium = b
        smallest = c
    else:
        largest = a
        medium = c
        smallest = b
elif b > a and b > c:
    if a > c:
        largest = b
        medium = a
        smallest = c
    else:
        largest = b
        medium = c
        smallest = a
elif c > a and c > b:
    if a > b:
        largest = c
        medium = a
        smallest = b
    else:
        largest = c
        medium = b
        smallest = a
print(largest)
print(medium)
print(smallest)