a = int(input())
a1 = a // 1000
a2 = (a // 100) % 10
a3 = (a // 10) % 10
a4 = a % 10
b1 = int(a1 * 1000 + a2 * 100 + a3 * 10 + a4)
b2 = int(a1 * 1000 + a2 * 100 + a4 * 10 + a3)
b3 = int(a1 * 1000 + a3 * 100 + a2 * 10 + a4)
b4 = int(a1 * 1000 + a3 * 100 + a4 * 10 + a2)
b5 = int(a1 * 1000 + a4 * 100 + a2 * 10 + a3)
b6 = int(a1 * 1000 + a4 * 100 + a3 * 10 + a2)
b7 = int(a2 * 1000 + a1 * 100 + a3 * 10 + a4)
b8 = int(a2 * 1000 + a1 * 100 + a4 * 10 + a3)
b9 = int(a2 * 1000 + a3 * 100 + a1 * 10 + a4)
b10 = int(a2 * 1000 + a3 * 100 + a4 * 10 + a1)
b11 = int(a2 * 1000 + a4 * 100 + a1 * 10 + a3)
b12 = int(a2 * 1000 + a4 * 100 + a3 * 10 + a1)
b13 = int(a3 * 1000 + a1 * 100 + a2 * 10 + a4)
b14 = int(a3 * 1000 + a1 * 100 + a4 * 10 + a2)
b15 = int(a3 * 1000 + a2 * 100 + a1 * 10 + a4)
b16 = int(a3 * 1000 + a2 * 100 + a4 * 10 + a1)
b17 = int(a3 * 1000 + a4 * 100 + a1 * 10 + a2)
b18 = int(a3 * 1000 + a4 * 100 + a2 * 10 + a1)
b19 = int(a4 * 1000 + a1 * 100 + a2 * 10 + a3)
b20 = int(a4 * 1000 + a1 * 100 + a3 * 10 + a2)
b21 = int(a4 * 1000 + a2 * 100 + a1 * 10 + a3)
b22 = int(a4 * 1000 + a2 * 100 + a3 * 10 + a1)
b23 = int(a4 * 1000 + a3 * 100 + a1 * 10 + a2)
b24 = int(a4 * 1000 + a3 * 100 + a2 * 10 + a1)
b1 += round(abs(b1 / 10000 - 0.6)) * 9999
b2 += round(abs(b2 / 10000 - 0.6)) * 9999
b3 += round(abs(b3 / 10000 - 0.6)) * 9999
b4 += round(abs(b4 / 10000 - 0.6)) * 9999
b5 += round(abs(b5 / 10000 - 0.6)) * 9999
b6 += round(abs(b6 / 10000 - 0.6)) * 9999
