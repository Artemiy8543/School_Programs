b14 += round(abs(b14 / 10000 - 0.6)) * 9999
b15 += round(abs(b15 / 10000 - 0.6)) * 9999
b16 += round(abs(b16 / 10000 - 0.6)) * 9999
b17 += round(abs(b17 / 10000 - 0.6)) * 9999
b18 += round(abs(b18 / 10000 - 0.6)) * 9999
b19 += round(abs(b19 / 10000 - 0.6)) * 9999
b20 += round(abs(b20 / 10000 - 0.6)) * 9999
b21 += round(abs(b21 / 10000 - 0.6)) * 9999
b22 += round(abs(b22 / 10000 - 0.6)) * 9999
b23 += round(abs(b23 / 10000 - 0.6)) * 9999
b24 += round(abs(b24 / 10000 - 0.6)) * 9999
c1 = min(b1, b2, b3, b4, b5, b6)
c2 = min(b7, b8, b9, b10, b11, b12)
c3 = min(b13, b14, b15, b16, b17, b18)
