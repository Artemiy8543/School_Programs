a = int(input())
b = int(input())
c = int(input())
for i in range(1, c + 1):
    if a >= b:
        a -= b
        print("Чашечка", i, "полная. В чайнике осталось", str(a) + ".")
    else:
        if a == 0:
            print("Чашечка", i, "пустая. В чайнике осталось 0.")
        else:
            a = 0
            print("Чашечка", i, "неполная. В чайнике осталось 0.") 